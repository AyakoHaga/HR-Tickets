        <script><![CDATA[/*
	サブカテゴリのvalueを取得
	
	既知の不具合により、変数のタイプが「Lookup select box」だとソートが効かない。
	https://hi.service-now.com/kb_view.do?sysparm_article=KB0695435
	回避策として変数のタイプを「参照」にしたが、選択時にsys_idを取得してしまうため、
	sys_idからvalueを取得する必要がある。その処理を行う。
*/
var subcategory = (function () {
	var gr = new GlideRecord('sys_choice');
	gr.addQuery('sys_id', (producer.subcategory.toString() || ''));
	gr.query();
    return gr.next() && gr.getRowCount() == 1 ? gr.getValue('value') : '';
}());

/*
	チケットテーブルにデータをマッピング
*/
current.setValue('caller_id', gs.getUserID());
current.setValue('subcategory', subcategory);
current.setValue('contact_type', '4');

/*
	処理完了後のリダイレクト先の設定
*/
var link = 
	'?id=form&table=' + current.getTableName() + '&view=ess&sys_id=' + current.getUniqueValue();
producer.portal_redirect = link;]]></script>
